https://www.codewars.com/kata/56ff6a70e1a63ccdfa0001b1/train/javascript

function arrayMadness(a, b) {
  let sum1 = 0;
  let sum2 = 0;
  for(let i = 0; i < a.length; i++) {
    sum1 += a[i] ** 2;
  }
  for(let j = 0; j < b.length; j++) {
    sum2 += b[j] ** 3;
  }
  return sum1 > sum2 ? true : false;
}
