function (pontoonDistance, sharkDistance, youSpeed, sharkSpeed, ){
  if () sharkSpeed = sharkSpeed / 2; 
    let sharkTime = sharkDistance / sharkSpeed;
    let yourTime = pontoonDistance / youSpeed;
  if (yourTime < sharkTime) return "Alive!" 
  else return " Bait!"
}
