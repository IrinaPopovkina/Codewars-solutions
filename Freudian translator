function toFreud(string) {
  if (string.length === 0) return '';
  let string1 = string.split(' ').map(el => 'sex').join(' ');
  return string1;
}


function toFreud(string) {
  return string === '' ? '' : 'sex '.repeat(string.split(' ').length).trim();
}
