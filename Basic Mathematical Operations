function basicOp(op, val1, val2) {
  let sum = val1 + val2;
  let dif = val1 - val2;
  let mult = val1 * val2;
  let div = val1 / val2;
    switch (op) {
      ase "+": return sum;
      case "-": return dif;
      case "*": return mult;
      case "/": return div;
   }
}
