function tacofy(word) {
  word = word.toLowerCase();
  let dct = {'t':'tomato', 'l':'lettuce',
             'c': 'cheese', 'g':'guacamole',
             's':'salsa', 'a':'beef', 'o':'beef', 'i':'beef', 'u':'beef',
             'e':'beef'};
  let arr = ['shell'];
  for(let el of word) {
    if (el in dct) {
      arr.push(dct[el]);
    }
  }
    arr.push('shell');
    return arr;
  }

